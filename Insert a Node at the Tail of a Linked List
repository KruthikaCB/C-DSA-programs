//https://www.hackerrank.com/challenges/insert-a-node-at-the-tail-of-a-linked-list/problem?isFullScreen=true
SinglyLinkedListNode* insertNodeAtTail(SinglyLinkedListNode* head, int data) {
SinglyLinkedListNode* pres=head;

SinglyLinkedListNode* temp=(SinglyLinkedListNode*)malloc(sizeof(SinglyLinkedListNode));
temp->data=data;
temp->next=NULL;

if(head==NULL){
    return temp;
}
while(pres->next!=NULL){
    
    pres=pres->next;
}
pres->next=temp;
return head;
}
